{
  "name": "ravenest-server",
  "version": "1.0.0",
  "description": "RaveNest backend server",
  "main": "src/index.ts",
  "scripts": {
    "start": "node dist/index.js",
    "server": "nodemon src/index.ts",
    "client": "cd ../client && npm run dev",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "tsc && cd ../client && npm run build && cp -r dist ../server/public",
    "postinstall": "tsc",
    "test": "jest --config jest.config.js --forceExit",
    "test:watch": "jest --watch",
    "seed": "ts-node src/seeds/index.ts"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "bcrypt": "^5.1.1",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "path": "^0.12.7",
    "pg": "^8.13.3",
    "pg-hstore": "^2.3.4",
    "react-router-dom": "^7.3.0",
    "sequelize": "^6.37.6"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^18.15.11",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.2",
    "dotenv": "^16.4.7",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
